{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "property": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::property.property",
      "inversedBy": "reviews"
    },
    "text": {
      "type": "text",
      "required": true
    },
    "rating": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 5
    },
    "userName": {
      "type": "string"
    },
    "userPhoto": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
