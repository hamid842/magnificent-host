      "target": "plugin::users-permissions.user",
      "inversedBy": "bookings"
    },
    "arrivalDate": {
      "type": "datetime"
    },
    "departureDate": {
      "type": "datetime"
    },
    "totalNight": {
      "type": "integer"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "booking"
    }
